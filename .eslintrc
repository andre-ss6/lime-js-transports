{
    'rules': {
        'indent': [2, 4],
        'quotes': [2, 'single'],
        'linebreak-style': [2, 'unix'],
        'semi': [2, 'always'],
        'comma-style': [2, 'last'],
        'comma-dangle': [2, 'only-multiline'],
        'no-unused-vars': [1],
    },
    'env': {
        'browser': true,
        'node': true,
        'amd': true,
    },
    'extends': 'eslint:recommended'
}
